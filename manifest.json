{
    "manifest_version": 3,
    "name": "CodeSafe",
    "description": "Identify and promote awareness of security bugs on web pages.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "icons": {
      "48": "icons/icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    }
  }
  